{
  "name": "vsix-viewer",
  "displayName": "VSIX Viewer",
  "description": "Viewer for .vsix extension packages",
  "version": "0.0.1",
  "publisher": "connor4312",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:connor4312.vsixViewer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/connor4312/vsix-viewer.git"
  },
  "enableProposedApi": true,
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "connor4312.vsixViewer",
        "displayName": "VSIX Viewer",
        "selector": [
          {
            "filenamePattern": "*.vsix"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && webpack",
    "compile": "tsc -p ./ && node create-template",
    "lint": "eslint src --ext ts",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:ts": "tsc -watch -p ./",
    "watch:pug": "chokidar src/webview.pug -c \"node create-template\"",
    "updatetypes": "cd src/types && vscode-dts dev && vscode-dts master",
    "pretest": "npm run compile && npm run lint"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.12.39",
    "@types/xml2js": "^0.4.5",
    "@types/yauzl": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "pug": "^2.0.4",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "execa": "^4.0.2",
    "xml2js": "^0.4.23",
    "yauzl": "^2.10.0"
  }
}
